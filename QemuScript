qemu-system-x86_64 \
	-name debian \
	-enable-kvm \
	-cpu host,kvm=on \
	-smp cores=8,threads=2,sockets=1 \
	-m 12G \
	-hda /nfs/sgoinfre/goinfre/Perso/yachaab/machine/debian.img \
	-device virtio-balloon \
	-nic user,model=virtio-net-pci,id=net0,hostfwd=tcp::10022-:22 \
	-device intel-hda \
	-device virtio-vga-gl \
	-display gtk,gl=on,show-cursor=on \
	-k en-us \
	-machine type=q35